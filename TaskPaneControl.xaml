<UserControl x:Class="QuickFile.TaskPaneControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuickFile"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Width="400">
    <StackPanel Background="White" >
        <Label Content="Hello"/>
        <TextBlock x:Name="textBlock"  Text="Hello"/>
        <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="" TextChanged="TextBox_TextChanged"/>
        <ListBox x:Name="listBox" MinHeight="100">
 
        </ListBox>
        <Button x:Name="button" Content="Button" Click="Button_Click"/>
    </StackPanel>
</UserControl>
